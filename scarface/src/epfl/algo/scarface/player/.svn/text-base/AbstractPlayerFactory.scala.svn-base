package epfl.algo.scarface.player

/**
 * An <code>AbstractPlayerFactory</code> represents a basic player factory object.
 */
trait AbstractPlayerFactory {

  import epfl.algo.scarface.gdl.Gdl._
  
  /**
   * Creates and returns a new <code>AbstractPlayer</code> with the provided arguments.
   */
  def playerNew(_gameId : String, _role : GdlAtom, _gameDescription : GdlList, _startClock : Int, _playClock : Int) : AbstractPlayer
  
}
